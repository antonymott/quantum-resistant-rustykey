    "test": "vitest --coverage",
    "lint": "tsc",
    "release": "changeset publish"
  },
  "engines": {
    "node": ">=23"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.3",
    "@vitest/coverage-v8": "^3.1.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
